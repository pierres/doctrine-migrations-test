{
    "name": "pierres/doctrine-migrations-test",
    "description": "Test cases to verify Doctrine Migrations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Schmitz",
            "email": "mail@pierre-schmitz.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "symfony/browser-kit": "^5.0",
        "symfony/framework-bundle": "^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "DoctrineMigrationsTest\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
