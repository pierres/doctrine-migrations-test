{
    "name": "pierres/doctrine-migrations-test",
    "description": "Test cases to verify Doctrine Migrations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Schmitz",
            "email": "mail@pierre-schmitz.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "symfony/browser-kit": "^5.2|^6.0",
        "symfony/framework-bundle": "^5.2|^6.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.0.2",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpunit/phpunit": "^9.5.9",
        "squizlabs/php_codesniffer": "^3.6.0"
    },
    "autoload": {
        "psr-4": {
            "DoctrineMigrationsTest\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
